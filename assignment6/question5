#include<iostream>
using namespace std;

struct Node{
    int data;
    Node *next;
};

Node *head = NULL;

void insert(int x){
    Node *n = new Node{x,NULL};
    if(head == NULL){
        head = n;
    } else{
        Node *t = head;
        while(t->next) t = t->next;
        t->next = n;
    }
}


void makeCircular(){
    if(head == NULL) return;
    Node *t = head;
    while(t->next) t = t->next;
    t->next = head;
}

bool isCircular(){
    if(head == NULL) return false;
    Node *t = head->next;
    while(t != NULL && t != head) t = t->next;
    return t == head;
}

int main(){
    int n, x;
    cout<<"Enter number of nodes: ";
    cin>>n;

    for(int i=0;i<n;i++){
        cin>>x;
        insert(x);
    }

    cout<<"Make the list circular? (1 for yes, 0 for no): ";
    int ch;
    cin>>ch;
    if(ch == 1) makeCircular();

    if(isCircular()) cout<<"Circular Linked List\n";
    else cout<<"Not Circular\n";

    return 0;
}
