#include<iostream>
using namespace std;

int heapArr[100];
int heapSize = 0;

void swap_(int &a, int &b){
    int t = a;
    a = b;
    b = t;
}

void heapifyUp(int i){
    while(i > 0){
        int p = (i - 1) / 2;
        if(heapArr[p] < heapArr[i]){
            swap_(heapArr[p], heapArr[i]);
            i = p;
        } else break;
    }
}

void heapifyDown(int i){
    while(true){
        int left = 2*i + 1;
        int right = 2*i + 2;
        int largest = i;
        if(left < heapSize && heapArr[left] > heapArr[largest]) largest = left;
        if(right < heapSize && heapArr[right] > heapArr[largest]) largest = right;
        if(largest != i){
            swap_(heapArr[i], heapArr[largest]);
            i = largest;
        } else break;
    }
}

void insertPQ(int x){
    heapArr[heapSize] = x;
    heapifyUp(heapSize);
    heapSize++;
}

int getMax(){
    if(heapSize == 0) return -1;
    return heapArr[0];
}

int deleteMax(){
    if(heapSize == 0) return -1;
    int maxi = heapArr[0];
    heapArr[0] = heapArr[heapSize - 1];
    heapSize--;
    heapifyDown(0);
    return maxi;
}

void displayPQ(){
    if(heapSize == 0){
        cout<<"Empty\n";
        return;
    }
    for(int i=0;i<heapSize;i++) cout<<heapArr[i]<<" ";
    cout<<endl;
}

int main(){
    int ch, x;
    while(true){
        cout<<"\n1 Insert\n";
        cout<<"2 Get Max\n";
        cout<<"3 Delete Max\n";
        cout<<"4 Display\n";
        cout<<"5 Exit\n";
        cin>>ch;
        if(ch == 1){
            cin>>x;
            insertPQ(x);
        } else if(ch == 2){
            x = getMax();
            if(x == -1) cout<<"Empty\n";
            else cout<<x<<endl;
        } else if(ch == 3){
            x = deleteMax();
            if(x == -1) cout<<"Empty\n";
            else cout<<x<<endl;
        } else if(ch == 4){
            displayPQ();
        } else break;
    }
    return 0;
}
